generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id         String   @id @default(uuid())
  name       String
  email      String   @unique
  password   String
  created_at DateTime @default(now())
  updated_at DateTime @default(now())
  phones     Phone[]

  @@map("users")
}

model Phone {
  id     String @id @default(uuid())
  userId String
  number String
  ddd    String

  user User @relation(fields: [userId], references: [id])

  @@map("phones")
}
